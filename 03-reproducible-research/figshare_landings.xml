                  </codeDefinition>
                  <codeDefinition>
                    <code>NPA</code>
                    <definition>Engraulis mordax</definition>
                  </codeDefinition>
                </enumeratedDomain>
              </nonNumericDomain>
            </nominal>
          </measurementScale>
        </attribute>
      </attributeList>
