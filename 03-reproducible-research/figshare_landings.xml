              <unit>
                <standardUnit>tonnes</standardUnit>
              </unit>
              <numericDomain>
                <numberType>real</numberType>
              </numericDomain>
            </ratio>
          </measurementScale>
        </attribute>
        <attribute>
          <attributeName>year</attributeName>
          <attributeDefinition>the year for which data was reported</attributeDefinition>
          <measurementScale>
            <dateTime>
              <formatString>YYYY</formatString>
            </dateTime>
          </measurementScale>
        </attribute>
        <attribute>
          <attributeName>a3_code</attributeName>
          <attributeDefinition>a 3 digit country code</attributeDefinition>
          <measurementScale>
            <nominal>
              <nonNumericDomain>
                <enumeratedDomain>
