    <dataTable>
      <entityName>Landings Data</entityName>
      <entityDescription>Landings data for several species by year, from the OpenFisheries database</entityDescription>
      <physical>
        <objectName>Landings_Data.csv</objectName>
        <size>9768</size>
        <characterEncoding>en_US.UTF-8</characterEncoding>
        <dataFormat>
          <textFormat>
            <numHeaderLines>1</numHeaderLines>
            <numFooterLines>0</numFooterLines>
            <recordDelimiter>\r\n</recordDelimiter>
            <attributeOrientation>column</attributeOrientation>
            <simpleDelimited>
              <fieldDelimiter>,</fieldDelimiter>
            </simpleDelimited>
          </textFormat>
        </dataFormat>
        <distribution>
          <online>
            <url function="download">http://files.figshare.com/1251242/Landings_Data.csv</url>
          </online>
        </distribution>
      </physical>
      <attributeList>
        <attribute>
          <attributeName>catch</attributeName>
          <attributeDefinition>Global Landings of fish</attributeDefinition>
          <measurementScale>
